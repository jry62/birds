    "    ix = birddata['bird_name'] == name\n",
    "    x, y = birddata.longitude[ix], birddata.latitude[ix]\n",
    "    ax.plot(x,y,'-', transform=ccrs.Geodetic(), label=name)\n",
    "    \n",
    "plt.legend(loc= \"upper left\")\n",
    "plt.savefig('map.pdf')"
   ]
